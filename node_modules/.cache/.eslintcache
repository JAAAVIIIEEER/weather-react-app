>\n    );\n  }\n  \n  \n\n  return (\n    <div className=\"app\">\n      <div className=\"search\">\n        <Button value=\"Palencia\"></Button>\n        <Button value=\"Maribor\"></Button>\n      </div>\n      <div className=\"container2\">\n        <div className=\"container\">\n          <div className=\"top\">\n            <div className=\"location\">\n              <p>{data.name}</p>\n            </div>\n            <div className=\"temp\">\n              {data.main ? <h1>{data.main.temp.toFixed()}°C</h1> : null}\n            </div>\n            <div className=\"description\">\n              {data.weather ? <p>{data.weather[0].main}</p> : null}\n            </div>\n          </div>\n\n          {data.name !== undefined &&\n            <div className=\"bottom\">\n              <div className=\"feels\">\n                {data.main ? <p className='bold'>{data.main.feels_like.toFixed()}°C</p> : null}\n                <p>Feels Like</p>\n              </div>\n              <div className=\"humidity\">\n                {data.main ? <p className='bold'>{data.main.humidity}%</p> : null}\n                <p>Humidity</p>\n              </div>\n              <div className=\"wind\">\n                {data.wind ? <p className='bold'>{data.wind.speed.toFixed() * 3.6} M/S</p> : null}\n                <p>Wind Speed</p>\n              </div>\n            </div>\n          }\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/home/jp/Documents/ERASMUS/NCC/A3_CCN/weather-react-app/src/Maps.js",["22","23","24"],[],"import { MapContainer, TileLayer , Popup, Marker} from 'react-leaflet'\nimport React from 'react'\nimport { Icon } from 'leaflet';\n\nfunction Maps() {\n    return (\n        <MapContainer center={[45.4, -75.7]} zoom={12}scrollWheelZoom={false}>\n      <TileLayer\n    url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n    attribution='&copy; <a href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors'\n      />\n    </MapCont